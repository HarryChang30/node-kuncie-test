{
  "name": "kuncie-checkout-service",
  "version": "1.0.0",
  "description": "Backend Kuncie Checkout Service",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon src/index.js",
    "lint": "eslint --fix ***/**/*.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "test": "mocha \"test/**/**/*.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HarryChang30/node-kuncie-test.git"
  },
  "keywords": [
    "kuncie",
    "checkout-service"
  ],
  "author": "Harry Chang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HarryChang30/node-kuncie-test/issues"
  },
  "homepage": "https://github.com/HarryChang30/node-kuncie-test#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "sequelize-cli": "^6.2.0",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "graphql-tools": "^8.2.0",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5"
  }
}
